/**index.wxss**/
.container{
    background: url('http://www.happyhao.top/res/bg01.png');
    position: fixed;
    width:100%;
    height:100%;
}
.logo image{
    width:357rpx;
    height:892rpx;
}
.title image{
    width:396rpx;
    height:66rpx;
    margin-top:52rpx;
}
swiper{
    width:100%;
    position: relative;
    height:calc(100% - 118rpx);
}
swiper swiper-item.active{
    /* background: url('http://www.happyhao.top/res/bg01.png'); */
}
swiper swiper-item .logo{
    display:flex;
    align-items: center;
    justify-content:center;
    width:100%;
    height:100%;
}
.item .bottomImg{
     width:100%;
     position: absolute;
     bottom:-100rpx;
}
.btn-group{
    padding:40rpx 100rpx;
    display:flex;
    justify-content: space-between;
}
.btn-group image{
    width:166rpx;
    height:66rpx;
}
.lastBtn{
    width:296rpx;
    height:113rpx;
}
.model{
     position:fixed;
     width:100%;
     height:100%;
     overflow:hidden;
     z-index:99;
     top:0;
     transition:all 2s ease-out 1s;
}
.model.active{
    transform: translateY(-100%); 
}
.model image{
     width:100%;
     margin-top:-16%;
}